<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>ServiceStack.OrmLite.Core</PackageId>
    <AssemblyName>ServiceStack.OrmLite</AssemblyName>
    <RootNamespace>ServiceStack.OrmLite</RootNamespace>
    <TargetFramework>net8.0</TargetFramework>
    <Title>ServiceStack.OrmLite</Title>
    <PackageDescription>
      ServiceStack.OrmLite
    </PackageDescription>
    <PackageTags>OrmLite;RDBMS;SQL;POCO;Code-First;ORM;Schema-less;Blobs</PackageTags>
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
	
	<ItemGroup>
    <PackageReference Include="SimdLinq" Version="1.3.2" />
    <PackageReference Include="System.Diagnostics.DiagnosticSource" Version="8.0.1" />
    <PackageReference Include="System.Dynamic.Runtime" Version="4.3.0" />
    <PackageReference Include="System.Reflection.Emit" Version="4.7.0" />
    <PackageReference Include="System.Reflection.Emit.Lightweight" Version="4.7.0" />
    <PackageReference Include="System.Reflection.TypeExtensions" Version="4.7.0" />
    <PackageReference Include="System.Collections.NonGeneric" Version="4.3.0" />
    <PackageReference Include="System.Xml.XmlDocument" Version="4.3.0" />
    <PackageReference Include="System.ComponentModel.Primitives" Version="4.3.0" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="6.0.0-preview.4.21253.7" />
    <PackageReference Include="System.Memory" Version="4.5.5" />
  </ItemGroup>
	
	<ItemGroup>
	  <Compile Update="Base\Common\MiniProfiler\Data\ProfiledCommand.cs">
	    <SubType>Component</SubType>
	  </Compile>
	  <Compile Update="Base\Common\MiniProfiler\Data\ProfiledConnection.cs">
	    <SubType>Component</SubType>
	  </Compile>
	</ItemGroup>

</Project>
